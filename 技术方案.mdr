    public List<TestResult> generateTestResults(Long testCaseId, int count) {
        List<TestResult> results = new ArrayList<>();
        TestStatus[] statuses = {TestStatus.PASSED, TestStatus.FAILED, TestStatus.SKIPPED};
        double[] probabilities = {0.8, 0.15, 0.05}; // é€šè¿‡ç‡80%ï¼Œå¤±è´¥15%ï¼Œè·³è¿‡5%
        
