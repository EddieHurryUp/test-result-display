    private TestStatus selectStatusByProbability(TestStatus[] statuses, double[] probabilities, Priority priority) {
        // é«˜ä¼˜å…ˆçº§ç”¨ä¾‹é€šè¿‡ç‡æ›´é«˜
        double[] adjustedProbabilities = Arrays.copyOf(probabilities, probabilities.length);
        if (priority == Priority.HIGH) {
