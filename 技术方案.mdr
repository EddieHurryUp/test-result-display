    // è®¡ç®—æ•´ä½“ç»Ÿè®¡ä¿¡æ¯
    public StatisticsSummary getSummary(Long projectId) {
        StatisticsSummary summary = new StatisticsSummary();
        
        // é¡¹ç›®ç»Ÿè®¡
        long projectCount = projectId == null ? 
            projectRepository.count() : 1;
        summary.setTotalProjects(projectCount);
        
        // ç”¨ä¾‹ç»Ÿè®¡
        long testCaseCount = projectId == null ?
            testCaseRepository.count() :
            testCaseRepository.countByProjectId(projectId);
        summary.setTotalTestCases(testCaseCount);
        
        // æ‰§è¡Œç»Ÿè®¡
        long totalExecutions = projectId == null ?
            testResultRepository.count() :
            testResultRepository.countByProjectId(projectId);
        summary.setTotalExecutions(totalExecutions);
        
        // é€šè¿‡ç‡è®¡ç®—
        if (totalExecutions > 0) {
            long passedCount = projectId == null ?
                testResultRepository.countByStatus(TestStatus.PASSED) :
                testResultRepository.countByStatusAndProject(TestStatus.PASSED, projectId);
            summary.setOverallPassRate((double) passedCount / totalExecutions * 100);
        }
        
        // å¹³å‡æ‰§è¡Œæ—¶é—´
        Double avgExecutionTime = projectId == null ?
            testResultRepository.getAverageExecutionTime() :
            testResultRepository.getAverageExecutionTimeByProject(projectId);
        summary.setAverageExecutionTime(avgExecutionTime != null ? avgExecutionTime.intValue() : 0);
        
        return summary;
    }
    
    // è·å–è¶‹åŠ¿æ•°æ®
    public TrendData getTrends(Long projectId, int days) {
        LocalDateTime startTime = LocalDateTime.now().minusDays(days);
        
        // æŒ‰æ—¥ç»Ÿè®¡é€šè¿‡ç‡è¶‹åŠ¿
        List<DailyTrend> dailyTrends = testResultRepository.findDailyTrends(projectId, startTime);
        
        // è®¡ç®—ç§»åŠ¨å¹³å‡çº¿
        List<AverageTrend> movingAverages = calculateMovingAverage(dailyTrends, 7);
        
        return TrendData.builder()
            .dailyTrends(dailyTrends)
            .movingAverages(movingAverages)
            .build();
    }
    
    // è·å–å¤±è´¥åˆ†ææ•°æ®
    public FailureAnalysis getFailureAnalysis(Long projectId, int limit) {
        // è·å–å¤±è´¥åŸå› ç»Ÿè®¡
        List<FailureReason> failureReasons = testResultRepository.findFailureReasons(projectId, limit);
        
        // è·å–å¤±è´¥ç‡æœ€é«˜çš„æµ‹è¯•ç”¨ä¾‹
